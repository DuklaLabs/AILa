      "parameters": {
        "functionCode": "const out = [];\nfor (const item of items) {\n  const r = item.json;\n  out.push({\n    den_v_tydnu: r.day_of_week,\n    hodina: r.lesson,\n    kapacita: r.capacity,\n    obsazeno: 0\n  });\n}\nreturn [{ json: out }];"
      }
    }
